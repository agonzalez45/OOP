#include <iostream>
#include <fstream>
#include <string>
using namespace std;

string toLowerCase(string word){
	for(int i = 0; i<word.length(); i++)
	{
		word[i] = tolower(word[i]);
	}
	return word;
}

string removePunctuation(string word){
	for(int i = 0; i<word.length(); i++)
	{
		str.substr(0,4) == "many";
	}
}

int main()
{
	ifstream in("words.txt");
	int count=0;


	string word,user;
	cin >> user; 
	user = toLowerCase(user);

	while(in >> word)
	{
		word = toLowerCase(word);
		word = removePunctuation(word);

		cout << "Comparing: " << word << " to " << user << endl;
		if(user == word)
		{
			count++;
		}
		
	}
	cout << count << endl;
return 0;
}
